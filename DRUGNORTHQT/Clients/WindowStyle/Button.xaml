    <!--Link-->
    <ControlTemplate x:Key="LinkButtonTemplate" TargetType="Button">
        <TextBlock TextDecorations="Underline">
            <ContentPresenter />
        </TextBlock>
    </ControlTemplate>

    <Style x:Key="LinkButtonStyle" TargetType="Button">
        <Setter Property="Foreground" Value="{StaticResource DefaultButtonBackground}" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter Property="Foreground" Value="Red" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <!--End link-->

    <!--Flat Button style-->
    <Style x:Key="InvicoFlatButtonStyle" TargetType="{x:Type Button}">
        <Setter Property="FontFamily" Value="{DynamicResource DefaultFontFamily}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Foreground" Value="{DynamicResource DirectButtonForeground}"/>
        <Setter Property="FontSize" Value="{DynamicResource DirectContentFontSize}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1" Background="{TemplateBinding Property=Background}">
                        <Label VerticalContentAlignment="Center" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="Center" HorizontalAlignment="Center" Content="{TemplateBinding Property=Content}" FontFamily="{TemplateBinding Property=FontFamily}" Foreground="{TemplateBinding Property=Foreground}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{DynamicResource DefaultButtonHoverBackground}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Background" Value="{DynamicResource DefaultButtonDisableBackground}"/>
            </Trigger>
            <Trigger Property="IsFocused" Value="true">
                <Setter Property="BorderBrush" Value="{DynamicResource DefaultButtonDisableBackground}"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <!--Icon cho nút thiết bị-->
    <Style x:Key="ScanDeviceIcon" TargetType="{x:Type RadioButton}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type RadioButton}">
                    <Border x:Name="boder" Width="76" Height="86" CornerRadius="10" Background="Transparent">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="48"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <!--<Image Grid.Row="0" 
                                   Source="pack://application:,,,/INV.Framework.MCD.Library;component/Resources/iconscanner.ico" 
                                   Stretch="Uniform" Margin="5"/>-->
                            <Path x:Name="path" Width="40" Height="40" Stretch="Fill" 
                                  Fill="#03A9F4" Grid.Row="0"
                                  Data="M18,3H6V7H18M19,12A1,1 0 0,1 18,11A1,1 0 0,1 19,10A1,1 0 0,1 20,11A1,1 0 0,1 19,12M16,19H8V14H16M19,8H5A3,3 0 0,0 2,11V17H6V21H18V17H22V11A3,3 0 0,0 19,8Z"/>
                            <TextBlock TextAlignment="Center" x:Name="txt" Foreground="Gray" 
                                       Text="{TemplateBinding Property=Content}" 
                                       FontFamily="{DynamicResource DefaultFontFamily}" 
                                       FontSize="{DynamicResource DefaultContentFontSize}" Grid.Row="1" 
                                       HorizontalAlignment="Center" TextTrimming="WordEllipsis" 
                                       TextWrapping="Wrap"/>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="boder" Property="Background" Value="{DynamicResource DefaultButtonHoverBackground}"/>
                            <Setter TargetName="txt" Property="Foreground" Value="{DynamicResource DefaultButtonForeground}"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="true">
                            <Setter TargetName="boder" Property="Background" Value="{DynamicResource DefaultButtonBackground}"/>
                            <Setter TargetName="txt" Property="Foreground" Value="{DynamicResource DefaultButtonForeground}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Margin" Value="5"/>
        <Setter Property="GroupName" Value="DeviceGroup"/>
    </Style>

